<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd"> 
<web-app> 
	<display-name>Projet</display-name> 
	<servlet> 
		<servlet-name>AddFriend</servlet-name> 
		<servlet-class>servlet.AddFriend</servlet-class> 
	</servlet> 

	<servlet> 
		<servlet-name>AddMsg</servlet-name> 
		<servlet-class>servlet.AddMsg</servlet-class> 
	</servlet> 

	<servlet> 
		<servlet-name>CreateUser</servlet-name> 
		<servlet-class>servlet.CreateUser</servlet-class> 
	</servlet> 

	<servlet> 
		<servlet-name>DeleteFriend</servlet-name> 
		<servlet-class>servlet.DeleteFriend</servlet-class> 
	</servlet> 

	<servlet> 
		<servlet-name>DeleteMsg</servlet-name> 
		<servlet-class>servlet.DeleteMsg</servlet-class> 
	</servlet> 

	<servlet> 
		<servlet-name>ListMsg</servlet-name> 
		<servlet-class>servlet.ListMsg</servlet-class> 
	</servlet> 
	
	<servlet> 
		<servlet-name>ListFriend</servlet-name> 
		<servlet-class>servlet.ListFriend</servlet-class> 
	</servlet> 

	<servlet> 
		<servlet-name>Login</servlet-name> 
		<servlet-class>servlet.Login</servlet-class> 
	</servlet>

	<servlet> 
		<servlet-name>Logout</servlet-name> 
		<servlet-class>servlet.Logout</servlet-class> 
	</servlet>

	<servlet-mapping> 
		<servlet-name>AddFriend</servlet-name> 
		<url-pattern>/Friend/Add</url-pattern> 
	</servlet-mapping> 

	<servlet-mapping> 
		<servlet-name>AddMsg</servlet-name> 
		<url-pattern>/Message/Add</url-pattern> 
	</servlet-mapping>  

	<servlet-mapping>
		<servlet-name>CreateUser</servlet-name> 
		<url-pattern>/User/Create</url-pattern> 
	</servlet-mapping>  

	<servlet-mapping> 
		<servlet-name>DeleteFriend</servlet-name> 
		<url-pattern>/Friend/Delete</url-pattern>  
	</servlet-mapping>  

	<servlet-mapping> 
		<servlet-name>DeleteMsg</servlet-name> 
		<url-pattern>/Message/Delete</url-pattern> 
	</servlet-mapping> 

	<servlet-mapping>
		<servlet-name>ListMsg</servlet-name> 
		<url-pattern>/Message/List</url-pattern> 
	</servlet-mapping> 

	<servlet-mapping>
		<servlet-name>ListFriend</servlet-name> 
		<url-pattern>/User/List</url-pattern> 
	</servlet-mapping> 
	
	<servlet-mapping> 
		<servlet-name>Login</servlet-name> 
		<url-pattern>/User/Login</url-pattern> 
	</servlet-mapping> 

	<servlet-mapping> 
		<servlet-name>Logout</servlet-name> 
		<url-pattern>/User/Logout</url-pattern>  
	</servlet-mapping> 

</web-app>
